<h1>Most popular monkeys</h1>
<div class="monkeys">
    {{#each monkeys}}
    <div class="monkey">
        <h2>{{name}}</h2>
        <img src={{image}}>
        <button class='btnInfo'>Info</button>
        <p style="display: none" id={{id}}>
            {{info}}
        </p>
    </div>
    {{/each}}
</div>